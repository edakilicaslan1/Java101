import java.util.Scanner;

public class Baslangic {
    static void desen(int sayi) {
        System.out.print(sayi + " ");
        if (sayi <= 0) return;
        desen(sayi - 5);
        System.out.print(sayi + " ");
    }

    public static void main(String[] args) {
        int sayi;
        Scanner giris = new Scanner(System.in);
        System.out.printf("SayÄ± giriniz : ");
        sayi = giris.nextInt();
        desen(sayi);
    }
}

import java.util.Scanner;
public class Baslangic {
    public static void main(String[] args) {
        double kilo, boy;

        Scanner input = new Scanner(System.in);
        System.out.print("Kg cinsinden kilo bilginizi giriniz:  ");
        kilo = input.nextDouble();
        System.out.print("Metre cinsinden boy bilginizi giriniz:  ");
        boy = input.nextDouble();

        double vucutkiloindeksi = (kilo / (boy * boy));
        System.out.print("VÃ¼cut kilo indeksiniz:   " + vucutkiloindeksi);


    }

}

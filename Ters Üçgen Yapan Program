import java.util.Scanner;
public class Baslangic {
    public static void main(String[] args) {
        int i, j, k, basamakSayisi = 10;
        for (i=10; 0<i; i--) {
            for(j=i; j<10;j++) {
                System.out.print(" ");
            }
            for (k=1; k<=i; k++) {
                System.out.print("*");
                System.out.print(" ");
            }
            System.out.println(" ");
        }

    }
}


